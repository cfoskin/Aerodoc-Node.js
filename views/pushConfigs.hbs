<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title> Push Config
    </title>
    <link href="https://netdna.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">

    <body>
        <div class="btn-toolbar">
            <div class="btn-group">
                <a href="/" class="btn btn-primary" id="signout">Sign Out</a>
                <a href="/leads" class="btn btn-primary" id="signout">Leads</a>
            </div>
        </div>
</head>
<div class="page-header">
    <h3 class="text-center">
       Aerodoc 
      </h3>
</div>
<div class="container" id="container" align="middle">
    <div class="panel panel-primary" style="width: 75%" align="middle">
        <div class="panel-heading">
            <h3 class="panel-title">
            PushConfig List
          </h3>
        </div>
        <div class="panel-body" align="middle" style="max-height: 355;overflow-y: scroll;">
            <table class="table">
                <thead>
                    <tr>
                        <th>Server URL</th>
                        <th>PushAppication Id</th>
                        <th>Active</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each allPushConfigs}}
                    <tr>
                        <td> {{serverURL}}</td>
                        <td> {{pushApplicationId}} </td>
                        <td> {{active}} </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
        <a href="/newPushConfig" class="btn btn-primary" id="newPushConfig">Create New PushConfig</a>
        <br>
        <br>
    </div>
</div>
</body>

</html>
